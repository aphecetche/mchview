{
  "src/views/*.js": {
    "alternate": "src/views/css/{}.css"
  },
  "src/views/css/*.css": {
    "alternate": "src/views/{}.js"
  },
  "*.stories.js": {
    "alternate": [
      "{}.js",
      "{}.test.js"
    ]
  },
  "*.test.js": {
    "alternate": [
      "{}.stories.js",
      "{}.js"
    ]
  },
  "*.js": {
    "alternate": [
      "{}.test.js",
      "{}.stories.js"
    ]
  }
}
